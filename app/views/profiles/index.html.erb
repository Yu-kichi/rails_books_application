<h1>Profiles#index</h1>
<p>Find me in app/views/profiles/index.html.erb</p>

<% @profiles.each do |profile| %>
  <br><%= profile.id %><br>
  <%= profile.name %><br>
  <td><%= l(profile.created_at, format: :short) %></td>
       <td><%= link_to t("directory.link.Show"), profile %></td>
       <td><%= link_to t("directory.link.Destroy"), profile, method: :delete, data: { confirm: t("directorymodal.confirm") } %></td>
<% end %>
<div class="paginate mt-4"><%= paginate @profiles %></div>